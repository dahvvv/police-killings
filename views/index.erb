<div id='container' class="">
  <div id='map-one' class='map'> </div>

  <button class="home">heatmap</button>
  <button class="markers">markermap</button>
  <button class="age-heat">by age heat</button>
  <button class="age-marker">by age marker</button>
  <button class="unarmed">victim armed/unarmed</button>

  <form id="state-selector" action="">
    <input type="text" id="state" name="state" placeholder="...AK">
    <input type="submit" value="state view">
  </form>

  <form id="age-range">
    <label for="age-min">Age Min:</label>
    <input type="text" id="age-min" name="age-min" />
    <label for="age-max">Age Max:</label>
    <input type="text" id="age-max" name="age-max" />
    <input class="age-range-submit" type="submit" value="Submit Age Range">
  </form>

  <div class="program">
    <script type="template" id="pop-template">
      <%= erb :_popup_template %>
    </script>
  </div>

  <div id='chart-wrapper'>
    <div id="infovis"></div>
  </div>
</div>
